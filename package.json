    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "lint-staged": "^7.0.4",
    "lodash": "^4.17.11",
    "prettier": "^1.12.0",
    "react-scripts": "^2.1.3",
    "typescript": "^3.0.0",
    "typescript-eslint-parser": "^17.0.0"
  },
  "dependencies": {
    "autoprefixer-stylus": "0.14.0",
    "express": "^4.16.4",
